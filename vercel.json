{
  "version": 2,
  "builds": [
    {
      "src": "api/route.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.10",
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/route.py",
      "methods": ["POST"]
    }
  ]
}
