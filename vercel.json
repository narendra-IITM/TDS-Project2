{
  "version": 2,
  "builds": [
    {
      "src": "api/route.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": ["api/*", "requirements.txt"],
        "excludeFiles": "**/*"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/route.py"
    }
  ]
}
